		"forge": "^2.3.0",
		"form-data": "^2.1.2",
		"jsonld": "^1.8.1",
		"node-forge": "^0.6.47",
		"pem-jwk": "^1.5.1",
		"pure-uuid": "^1.6.2",
		"request": "^2.88.2",
		"text-encoding": "^0.7.0",
		"xmlhttprequest": "^1.8.0"
	},
