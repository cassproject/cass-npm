{
	"name": "cassproject",
	"version": "4.0.0",
	"description": "Competency and Skills Service",
	"main": "output.js",
	"scripts": {
		"test": "npm run build && npm run test14 && npm run test13 && npm run test12",
		"test14": "npm run testkill; docker run -d --name cass-test -p80:80 cassproject/cass:alpine-1.4.0 && npm run testrun14 && node test.js && npm run testkill",
		"test13": "npm run testkill; docker run -d --name cass-test -p80:80 cassproject/cass:alpine-1.3.15 && npm run testrun13 && node test.js && npm run testkill",
		"test12": "npm run testkill; docker run -d --name cass-test -p80:80 cassproject/cass:alpine-1.2.40 && npm run testrun12 && node test.js && npm run testkill",
		"testkill": "docker kill cass-test && docker rm cass-test",
		"build": "node process.js",
		"publish": "npm publish",
		"docs": "yuidoc -c yuidoc.json -o docs -e .js src"
	},
	"bin": {
		"cassSetServer": "bin/cassSetServer",
		"cassListFrameworks": "bin/cassListFrameworks"
	},
	"contributors": [
